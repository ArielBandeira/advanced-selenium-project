<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="PositiveLoginTestSuite" verbose="1">

    <test name="PositiveLoginTestWithProfile">
        <parameter name="firefoxProfile" value="FirefoxProfile" />
        <classes>
            <class name="com.herokuapp.theinternet.loginPageTests.PositiveLoginTests" />
        </classes>
    </test>

    <test name="PositiveLoginTestWithoutProfile">
        <classes>
            <class name="com.herokuapp.theinternet.loginPageTests.PositiveLoginTests" />
        </classes>
    </test>

</suite>